      REAL*8 ZERO,HALF,ONE,TWO,MONE,GAM,GM1,GOGM1,GINV,GM1OG,GP1OG
      REAL*8 PI,RGAS
C
      PARAMETER (ZERO = 0.00d0 , HALF = 0.5d0 , ONE = 1.00d0 , 
     . TWO = 2.00d0 , MONE = -ONE, GAM = 1.40d0 , GM1  = GAM - ONE , 
