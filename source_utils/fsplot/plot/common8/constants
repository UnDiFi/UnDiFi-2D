      REAL*8 ZERO,HALF,ONE,TWO,GAM,GM1,GOGM1,GINV,GM1OG,GP1OG
      REAL*8 PI,S0
C
      PARAMETER (ZERO = 0.00d0 , HALF = 0.5d0 , ONE = 1.00d0 , 
     . TWO = 2.00d0 , GAM = 1.40d0 , GM1  = GAM - ONE , 
     . GOGM1 = GAM / GM1, GINV = ONE / GAM , GM1OG = GM1 * GINV , 
     . GP1OG = (GAM+ONE)*GINV, PI = 3.14159265358979323846d0,
     . S0 = 110.5d0)

